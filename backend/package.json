    "@rokmohar/medusa-plugin-meilisearch": "1.2.3",
    "@sanity/client": "^7.8.1",
    "@woocommerce/woocommerce-rest-api": "^1.0.2",
    "awilix": "^8.0.1",
    "axios": "^1.11.0",
    "cheerio": "^1.1.2",
    "dotenv": "^17.2.1",
